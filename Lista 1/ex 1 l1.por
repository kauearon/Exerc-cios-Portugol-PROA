programa {
  funcao inicio() {
    cadeia nome_do_carro , fusca
    fusca = "fuska"

  escreva("Informe o nome do carro: ")
  leia(nome_do_carro)

  se(nome_do_carro != fusca){

    escreva("Você esta equivocado, seu carro é um fuska")
  }

  senao{

    escreva("Isso mesmo, seu carro é um fuska")
  }
  }
}
